# NEAR Account Configuration
# Replace these with your actual NEAR testnet account credentials

# Your NEAR testnet account ID (e.g., yourname.testnet)
ACCOUNT_ID=your-account.testnet

# Your NEAR account private key (ed25519 format)
PRIVATE_KEY=ed25519:your_private_key_here_replace_with_actual_64_character_key

# NEAR Chain Signature Contract
NEXT_PUBLIC_NEAR_CHAIN_SIGNATURE_CONTRACT=v1.signer-prod.testnet

# Optional: Additional NEAR Configuration
NEXT_PUBLIC_NEAR_NETWORK_ID=testnet
NEXT_PUBLIC_NEAR_ACCOUNT=your-account.near

# Optional: EVM Configuration (for future examples)
NEXT_PUBLIC_SEPOLIA_INFURA_URL=your_infura_url_here
NEXT_PUBLIC_RAINBOW_KIT_PROJECT_ID=your_rainbow_kit_project_id
